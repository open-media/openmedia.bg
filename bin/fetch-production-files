#!/bin/sh

production_server='openmedia@openmedia.bg'
production_project_root='/home/openmedia/openmedia.bg'
local_project_root="`dirname $0`/.."
uploads_path='web/app/uploads'

rsync -r --progress $production_server:"$production_project_root/shared/$uploads_path/" "$local_project_root/$uploads_path"

echo 'Done.'
